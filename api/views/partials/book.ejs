
<div class="card-event mdl-card mdl-shadow--2dp mdl-cell mdl-cell--3-col">
  <div class="mdl-card__title mdl-card--expand">
    <h4><%= book.title %></h4>
  </div>
  <div class="mdl-card__supporting-text">
    <%= book.author %>
  </div>
  <div class="mdl-card__actions mdl-card--border">
    <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" href="<%= book.link ? book.link() : '#' %>">
      Подробнее
    </a>
  </div>
</div>
